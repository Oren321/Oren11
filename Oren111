--AutoMine
local Toggle = Tabs.AutoMine:AddToggle("AutoMine", 
{
    Title = "AutoMine Dirt", 
    Description = "",
    Default = false,
    Callback = function(state)
        if state then
            if not getgenv().toggle then
                getgenv().toggle = true
                spawn(function()
                    while getgenv().toggle do
                        game:GetService("ReplicatedStorage").Network.Mining_SetState:InvokeServer(true)
                        game:GetService("ReplicatedStorage").Network.Mining_Attack:InvokeServer(6)
                        task.wait()
                    end
                end)
            end
        else
            getgenv().toggle = false
        end
    end
})
