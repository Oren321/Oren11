-- Required modules
local InventoryCmds = require(game.ReplicatedStorage.Library.Client.InventoryCmds)
local LockpickItem = require(game.ReplicatedStorage.Library.Items.LockpickItem)
local Save = require(game.ReplicatedStorage.Library.Client.Save)

-- Function to print all lockpick info
local function printAllLockpicks()
    -- Get save data first
    local saveData = Save.Get()
    local currentUID = saveData and saveData.SelectedLockpick and saveData.SelectedLockpick.uid

    print("Currently Selected Lockpick:", currentUID or "None")
    print("
All Available Lockpicks:")

    -- Get container
    local container = InventoryCmds.Container()
    if container then
        -- Loop through all items
        for _, item in pairs(container:GetAll()) do
            if item:IsA("LockpickItem") then
                local itemData = item:Directory()
                print("------------------------")
                print("UID:", item:GetUID())
                print("Type:", itemData.Name)
                print("Tier:", itemData.Order)
                print("Rarity:", itemData.Rarity.Name)
            end
        end
    end
end

-- Print all lockpicks
printAllLockpicks()
